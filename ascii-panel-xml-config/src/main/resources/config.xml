<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.springframework.org/schema/beans
          http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="asciiPanel" class="asciiPanel.AsciiPanel">
        <property name="asciiFont" ref="asciiFont"></property>
    </bean>

    <bean id="asciiFont" class="asciiPanel.AsciiFont">
        <constructor-arg type="java.lang.String" value="cp437_8x8.png"></constructor-arg>
        <constructor-arg type="int" value="8"></constructor-arg>
        <constructor-arg type="int" value="8"></constructor-arg>
    </bean>
</beans>